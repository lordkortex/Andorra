<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" 
                access="global" >
		<lightning:card class="responsiveCard" iconName="custom:custom101">
			<aura:set attribute="title">
          		<span title="{!$Label.c.CRAN_ICA_MKT_RG_Title}">{!$Label.c.CRAN_ICA_MKT_RG_Title}</span>
     		</aura:set>
			<aura:set attribute="actions">
				<lightning:button variant="brand"
					label="{!$Label.c.CRAN_ICA_MKT_RG_FilterTitle}"
					onclick="{!c.redirectToComponent}" />
			</aura:set>
			<p class="slds-m-horizontal--small slds-text-body--regular">{!$Label.c.CRAN_VAS_MAQ_RG_TitleInfo}</p>
		</lightning:card>
</aura:component>