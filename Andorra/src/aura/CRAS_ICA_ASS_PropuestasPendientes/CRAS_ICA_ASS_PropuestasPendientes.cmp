<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId" 
                access="global"
                controller="CRAS_ICA_ASS_Reports_AUC">
    
    <ltng:require scripts="{!join(',',
                           $Resource.jQuery3,
                           $Resource.highcharts_js,
                           $Resource.grouped_categories_js,
                           $Resource.exporting_js	
                           )}" 
                  afterScriptsLoaded="{!c.init}"/>
    <aura:attribute name="title1" type="String" default="{!$Label.c.CRAS_ICA_ASS_PendingProposals}"/>
    <aura:attribute name="legend1" type="String" default="{!$Label.c.CRAS_ICA_ASS_NumPol_Short}"/>

    <aura:attribute name="nodata1" type="String" default="{!$Label.c.CRAS_ICA_ASS_NODATA_PendingProposals}"/>
    
    <div class="slds-card">
        <div class="slds-card__header slds-grid">
            <header class="slds-media slds-media_center slds-has-flexi-truncate">
                <div class="slds-media__body">
	            	<h2><span class="slds-text-heading_small">{!v.title1}</span></h2>
                </div>
            </header>
        </div>
        <div class="slds-card__body slds-card__body_inner">
            <div id="container_PP" aura:id="container_RD" class="spinnerContainer">
                <!--<lightning:spinner aura:id="listSpinner" variant="brand" size="small" />-->
                <div class="loadGraphic">{!$Label.c.CRAS_ICA_ASS_LoadGraphic}</div>
            </div>
        </div>
    </div>
    
</aura:component>